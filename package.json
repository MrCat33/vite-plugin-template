  "dependencies": {
    "@types/node": "^18.6.1",
    "fast-glob": "^3.2.11"
  },
  "description": "autoExport typescript code",
  "devDependencies": {
    "bumpp": "^8.2.1",
    "esno": "^0.16.3",
